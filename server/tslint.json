{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "trailing-comma": [false],
    "object-literal-shorthand": true,
    "object-shorthand-properties-first": false
  },
  "rulesDirectory": []
}
